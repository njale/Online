<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.Statnett" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="180000" />
    <feature name="jsHybugger">
        <param name="android-package" value="org.jshybugger.cordova.JsHybuggerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Statnett</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Apache Cordova Team </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
    <vs:platformSpecificValues />
</widget>
